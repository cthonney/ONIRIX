<div class="blogs-container">
  <div style="margin-bottom: 30px;">
  <h1>IID Experts' Articles</h1>
  <p>Dream interpretation and analysis: read more about our expert tips and techniques</p>
  </div>
  <div class="blogs mx-auto">
    <% @blogs.each do |blog| %>
      <div class='blog card p-4 rounded-4 shadow-2'>
        <h2><%= blog[:title] %></h2>
        <p class='card-text'><%= blog[:content]%> </p>
      </div>
    <% end %>
  </div>
</div>
